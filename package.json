{
  "name": "airbus-showcase",
  "version": "1.0.1",
  "description": "Erlebe die Produkte und Leistungen von Airbus Bremen bei einer virtuellen Reise von Bremen bis ins All.",
  "main": "src/main.js",
  "scripts": {
    "build": "parcel build index.html --no-cache --public-url ./",
    "serve": "parcel index.html -p 8080 --no-hmr",
    "start": "npm run serve",
    "dev": "npm run serve",
    "lint": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaskuske/airbus-showcase.git"
  },
  "author": "Jonas Kuske",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonaskuske/airbus-showcase/issues"
  },
  "homepage": "https://github.com/jonaskuske/airbus-showcase#readme",
  "eslintIgnore": [
    "dist/",
    ".cache/",
    "static/animationData/"
  ],
  "prettierIgnore": [
    "static/animationData/"
  ],
  "assetsPath": "static",
  "dependencies": {
    "gsap": "^2.0.2",
    "lottie-web": "^5.4.3",
    "promise-polyfill": "^8.1.0",
    "scrollmagic": "^2.0.6",
    "smoothscroll-polyfill": "^0.4.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/runtime": "^7.3.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-asset-copier": "^1.0.0",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "prettier": "^1.16.4"
  }
}