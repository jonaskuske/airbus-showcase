{
  "name": "airbus-showcase",
  "version": "1.0.2",
  "description": "Erlebe die Produkte und Leistungen von Airbus Bremen bei einer virtuellen Reise von Bremen bis ins All.",
  "main": "src/main.js",
  "scripts": {
    "build": "parcel build index.html --no-cache --public-url /airbus/",
    "serve": "parcel index.html -p 8080 --no-hmr",
    "start": "npm run serve",
    "dev": "npm run serve",
    "lint": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaskuske/airbus-showcase.git"
  },
  "author": "Jonas Kuske",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonaskuske/airbus-showcase/issues"
  },
  "homepage": "https://github.com/jonaskuske/airbus-showcase#readme",
  "eslintIgnore": [
    "dist/",
    ".cache/",
    "static/animationData/"
  ],
  "prettierIgnore": [
    "static/animationData/"
  ],
  "assetsPath": "static",
  "dependencies": {
    "gsap": "^2.1.3",
    "lottie-web": "^5.5.4",
    "promise-polyfill": "^8.1.0",
    "scrollmagic": "^2.0.7",
    "smoothscroll-polyfill": "^0.4.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-asset-copier": "^1.0.0",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-compress": "^1.0.10",
    "prettier": "^1.18.2"
  }
}
